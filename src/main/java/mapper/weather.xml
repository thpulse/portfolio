<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kitri.pro.weather">

	<select id="arealist" resultType="area">
		select * from area where addr1 in ('02','05','06','07','08','09','10','11')
	</select>
		<insert id="weatherinsert" parameterType="weather">
		insert into weather values(#{days},#{max_temp},#{min_temp},#{info},#{addr1},#{addr2},#{addr3},#{addr4})
	</insert>
		<select id="lastweather" resultType="weather">
		 	select * from (select * from weather order by to_number(substr(days,1,4)) desc) where rownum=1
	</select>
</mapper>











